<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mySampleApplication.DAO.HellotableEntity" table="hellotable" schema="" catalog="helloworld">
        <id name="id">
            <column name="id" sql-type="smallint" not-null="true"/>
        </id>
        <property name="value">
            <column name="value" sql-type="varchar" length="20"/>
        </property>
    </class>
</hibernate-mapping>